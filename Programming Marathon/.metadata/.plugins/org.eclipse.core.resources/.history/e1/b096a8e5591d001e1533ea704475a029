package program7;

public class SemesterMarksAverage {
public static void main(String[] args) {
	int sem1=85;
	int sem2=79;
	int sem3=91;
	int sem4=76;
	int sem5=88;
	int sem6=95;
	int sem7=80;
	int sem8=85;
	double res=calculateAverage(sem1,sem2, sem3,sem4,sem5,sem6,sem7,sem8);
	System.out.println(res);
	
	
}
public static double calculateAverage(int sem1,int sem2,int sem3,int sem4,int sem5,int sem6,int sem7,int sem8) {
	double avg=sem1+sem2+ sem3+ sem4+ sem5+ sem6+sem7+ sem8;
	return avg;
}
}
